#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/esbuild/bin/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/esbuild/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules:/Volumes/konate/AUTRES/wiassur/node_modules:/Volumes/konate/AUTRES/node_modules:/Volumes/konate/node_modules:/Volumes/node_modules:/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/esbuild/bin/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/esbuild/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules:/Volumes/konate/AUTRES/wiassur/node_modules:/Volumes/konate/AUTRES/node_modules:/Volumes/konate/node_modules:/Volumes/node_modules:/node_modules:/Volumes/konate/AUTRES/wiassur/wiassur_front_engine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
